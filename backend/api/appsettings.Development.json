{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.Http.HttpClient": "Error",
        "HealthChecks.UI": "Error"
      }
    }
  },
  "ConnectionStrings": {
    "PIMS": "Server=database,1433;User ID=admin;Database=pims"
  },
  "Pims": {
    "Environment": {
      "Uri": "https://dev-pims.th.gov.bc.ca",
      "Name": "Development"
    },
    "Notifications": {
      "ThrowExceptions": true
    }
  }
}
