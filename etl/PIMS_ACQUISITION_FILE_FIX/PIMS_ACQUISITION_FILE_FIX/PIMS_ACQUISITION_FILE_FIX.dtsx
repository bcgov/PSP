      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="WITH CASE2 AS  &#xA;(&#xA;    SELECT&#xA;&#x9;&#x9;REGION_CODE,&#xA;        FILE_NO,&#xA;        FILE_NUMBER,&#xA;        ACQUISITION_FILE_ID,&#xA;        CASE &#xA;            WHEN PATINDEX('%[A-Z]%', SUBSTRING(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1) + 1, LEN(FILE_NUMBER))) &gt; 0 &#xA;            THEN 'YES'&#xA;            ELSE 'NO'&#xA;        END AS HasAlphabets&#xA;    FROM &#xA;        dbo.PIMS_ACQUISITION_FILE&#xA;    WHERE&#xA;        FILE_NUMBER NOT LIKE '%-R%' &#xA;        AND APP_CREATE_USERID = 'PAIMS_PIMS_ACQUISITION'&#xA;),&#xA;NUMBERED_FILES AS &#xA;(&#xA;    SELECT&#xA;&#x9;REGION_CODE,&#xA;        FILE_NO, &#xA;        FILE_NUMBER,&#xA;        ACQUISITION_FILE_ID,&#xA;        ROW_NUMBER() OVER (PARTITION BY REGION_CODE, FILE_NO ORDER BY FILE_NUMBER) AS NewSuffixNumber&#xA;    FROM &#xA;        CASE2&#xA;    WHERE &#xA;        HasAlphabets = 'YES'&#xA;&#x9;AND NOT ( LEN(FILE_NUMBER) &gt;= 2 AND ISNUMERIC(RIGHT(FILE_NUMBER, 1)) = 1 AND PATINDEX('%[A-Z]%', RIGHT(FILE_NUMBER, 2)) &gt; 0 )&#xA;),&#xA;UPDATED_FILES AS&#xA;(&#xA;    SELECT &#xA;&#x9;&#x9;REGION_CODE,&#xA;        FILE_NO, &#xA;        FILE_NUMBER, &#xA;        ACQUISITION_FILE_ID,&#xA;        LEFT(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1)) &#xA;        + RIGHT('00' + CAST(NewSuffixNumber AS VARCHAR(2)), 2) AS Updated_FILE_NUMBER,&#xA;        NewSuffixNumber&#xA;    FROM &#xA;        NUMBERED_FILES&#xA;)&#xA;SELECT &#xA;&#x9;UF.REGION_CODE,&#xA;    UF.FILE_NO, &#xA;    UF.FILE_NUMBER, &#xA;    UF.ACQUISITION_FILE_ID,&#xA;    UF.Updated_FILE_NUMBER,&#xA;    CASE &#xA;        WHEN UF.NewSuffixNumber = 1 THEN NULL  -- Parent file (Updated_FILE_NUMBER = &quot;01&quot;)&#xA;        ELSE PF.ACQUISITION_FILE_ID           -- Child file points to the ACQUISITION_FILE_ID of the parent file&#xA;    END AS PRNT_ACQUISITION_FILE_ID&#xA;&#xA;INTO etl.CASE2_ALPHABETS&#xA;&#xA;FROM &#xA;    UPDATED_FILES UF&#xA;LEFT JOIN &#xA;    UPDATED_FILES PF &#xA;    ON UF.FILE_NO = PF.FILE_NO &#xA;    AND PF.NewSuffixNumber = 1  -- Find the parent file with Updated_FILE_NUMBER = &quot;01&quot;&#xA;ORDER BY &#xA;    UF.FILE_NO, UF.NewSuffixNumber;&#xA;&#xA;&#x9; &#xA; &#xA;&#xA; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\CASE3_SINGLE_DIGITS"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{1D02BF51-ED3F-412A-A4A6-6000BE856EB0}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="CASE3_SINGLE_DIGITS"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="WITH CASE3 AS  &#xA;(&#xA;    SELECT&#xA;        ACQUISITION_FILE_ID,&#xA;        REGION_CODE,&#xA;        FILE_NO,&#xA;        FILE_NUMBER,&#xA;        LEFT(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1)) &#xA;        + '0' &#xA;        + SUBSTRING(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1) + 1, 1) AS UPDATED_FILE_NUMBER&#xA;    FROM &#xA;        dbo.PIMS_ACQUISITION_FILE&#xA;    WHERE &#xA;        LEN(SUBSTRING(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1) + 1, LEN(FILE_NUMBER))) = 1 &#xA;        AND ISNUMERIC(SUBSTRING(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1) + 1, 1)) = 1&#xA;        AND APP_CREATE_USERID = 'PAIMS_PIMS_ACQUISITION'&#xA;),&#xA;&#xA;NUMBERED_FILES AS &#xA;(&#xA;    SELECT &#xA;        REGION_CODE,&#xA;        FILE_NO, &#xA;        FILE_NUMBER,&#xA;        ACQUISITION_FILE_ID,&#xA;        ROW_NUMBER() OVER (PARTITION BY REGION_CODE, FILE_NO ORDER BY FILE_NUMBER) AS NewSuffixNumber&#xA;    FROM &#xA;        CASE3&#xA;),&#xA;&#xA;UPDATED_FILES AS&#xA;(&#xA;    SELECT &#xA;        REGION_CODE,&#xA;        FILE_NO, &#xA;        FILE_NUMBER, &#xA;        ACQUISITION_FILE_ID,&#xA;        LEFT(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1)) &#xA;        + RIGHT('00' + CAST(NewSuffixNumber AS VARCHAR(2)), 2) AS Updated_FILE_NUMBER,&#xA;        NewSuffixNumber&#xA;    FROM &#xA;        NUMBERED_FILES&#xA;)&#xA;&#xA;SELECT&#xA;    UF.REGION_CODE,&#xA;    UF.FILE_NO, &#xA;    UF.FILE_NUMBER, &#xA;    UF.ACQUISITION_FILE_ID,&#xA;    UF.Updated_FILE_NUMBER,&#xA;    CASE &#xA;        WHEN UF.NewSuffixNumber = 1 THEN NULL  -- Parent file (Updated_FILE_NUMBER = &quot;01&quot;)&#xA;        ELSE PF.ACQUISITION_FILE_ID           -- Child file points to the ACQUISITION_FILE_ID of the parent file (first row)&#xA;    END AS PRNT_ACQUISITION_FILE_ID&#xA;INTO etl.CASE3_SINGLE_DIGITS&#xA;FROM &#xA;    UPDATED_FILES UF&#xA;LEFT JOIN &#xA;    UPDATED_FILES PF &#xA;    ON UF.FILE_NO = PF.FILE_NO &#xA;    AND PF.NewSuffixNumber = 1  -- Ensure PF is the parent row with NewSuffixNumber = 1&#xA;&#x9;--WHERE UF.FILE_NO = 4613&#xA;ORDER BY &#xA;    UF.FILE_NO, UF.NewSuffixNumber;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
