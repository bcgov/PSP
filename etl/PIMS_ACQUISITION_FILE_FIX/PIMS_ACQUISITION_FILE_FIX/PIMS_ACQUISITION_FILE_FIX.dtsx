      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="-- Extract only the necessary records in a more efficient manner&#xA;WITH CASE5 AS  &#xA;(&#xA;    SELECT &#xA;&#x9;&#x9;REGION_CODE,&#xA;        FILE_NO,&#xA;        FILE_NUMBER,&#xA;        ACQUISITION_FILE_ID,&#xA;        CASE &#xA;            WHEN PATINDEX('%[A-Z]%', SUBSTRING(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1) + 1, LEN(FILE_NUMBER))) &gt; 0 &#xA;            THEN 'YES'&#xA;            ELSE 'NO'&#xA;        END AS HasAlphabetOnlyR&#xA;    FROM &#xA;        dbo.PIMS_ACQUISITION_FILE&#xA;    WHERE &#xA;        FILE_NUMBER LIKE '%-R'    -- Simplified condition to match only records ending with &quot;-R&quot;&#xA;        AND APP_CREATE_USERID = 'PAIMS_PIMS_ACQUISITION'&#xA;)&#xA;--SELECT * FROM CASE5 ORDER BY REGION_CODE, FILE_NO&#xA;&#xA;-- Group and filter FILE_NO values with more than one entry&#xA;,group_file_no AS (&#xA;    SELECT &#xA;&#x9;REGION_CODE,&#xA;        FILE_NO, &#xA;        COUNT(1) AS c &#xA;    FROM &#xA;        CASE5 &#xA;    GROUP BY &#xA;        REGION_CODE, FILE_NO &#xA;    HAVING &#xA;        COUNT(1) &gt; 1&#xA;)&#xA;&#xA;--SELECT * FROM group_file_no ORDER BY REGION_CODE, file_no&#xA;-- Use LEFT JOIN and IS NULL instead of NOT IN for better performance&#xA;,NUMBERED_FILES AS &#xA;(&#xA;    SELECT&#xA;&#x9;&#x9;CASE5.REGION_CODE,&#xA;        CASE5.FILE_NO, &#xA;        CASE5.FILE_NUMBER,&#xA;        CASE5.ACQUISITION_FILE_ID,&#xA;        ROW_NUMBER() OVER (PARTITION BY CASE5.REGION_CODE, CASE5.FILE_NO ORDER BY CASE5.FILE_NUMBER) AS NewSuffixNumber&#xA;    FROM &#xA;        CASE5&#xA; &#xA;    WHERE &#xA;        CASE5.HasAlphabetOnlyR = 'YES'&#xA;        AND CASE5.FILE_NO NOT IN ( SELECT FILE_NO FROM group_file_no )&#xA;)&#xA;&#xA;-- SELECT * FROM NUMBERED_FILES ORDER BY REGION_CODE, FILE_NO&#xA;&#xA;-- Generate updated file numbers&#xA;,UPDATED_FILES AS&#xA;(&#xA;    SELECT &#xA;&#x9;&#x9;REGION_CODE,&#xA;        FILE_NO, &#xA;        FILE_NUMBER, &#xA;        ACQUISITION_FILE_ID,&#xA;        LEFT(FILE_NUMBER, CHARINDEX('-', FILE_NUMBER, CHARINDEX('-', FILE_NUMBER) + 1)) &#xA;        + RIGHT('00' + CAST(NewSuffixNumber AS VARCHAR(2)), 2) AS Updated_FILE_NUMBER,&#xA;        NewSuffixNumber&#xA;    FROM &#xA;        NUMBERED_FILES&#xA;)&#xA;-- SELECT * FROM UPDATED_FILES ORDER BY REGION_CODE, FILE_NO&#xA;-- Final selection&#xA;SELECT &#xA;    UF.FILE_NO, &#xA;    UF.FILE_NUMBER, &#xA;    UF.ACQUISITION_FILE_ID,&#xA;    UF.Updated_FILE_NUMBER,&#xA;    CASE &#xA;        WHEN UF.NewSuffixNumber = 1 THEN NULL  -- Parent file (Updated_FILE_NUMBER = &quot;01&quot;)&#xA;        ELSE PF.ACQUISITION_FILE_ID           -- Child file points to the ACQUISITION_FILE_ID of the parent file&#xA;    END AS PRNT_ACQUISITION_FILE_ID&#xA;&#xA;INTO etl.CASE5_ALPHABET_R&#xA;&#xA;FROM &#xA;    UPDATED_FILES UF&#xA;LEFT JOIN &#xA;    UPDATED_FILES PF &#xA;    ON UF.FILE_NO = PF.FILE_NO &#xA;    AND PF.NewSuffixNumber = 1  -- Join to find the parent file with Updated_FILE_NUMBER = &quot;01&quot;&#xA;ORDER BY &#xA;    UF.FILE_NO, UF.NewSuffixNumber;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
