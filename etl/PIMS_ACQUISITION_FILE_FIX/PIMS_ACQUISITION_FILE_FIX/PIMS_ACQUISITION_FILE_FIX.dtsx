      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="-- IDENTIFY RECORDS THAT HAVE SUB FILES&#xA;WITH CASE6 AS  &#xA;(&#xA;    SELECT &#xA;&#x9;&#x9;REGION_CODE,&#xA;        FILE_NO,&#xA;        FILE_NUMBER,&#xA;&#x9;&#x9;LEGACY_FILE_NUMBER,&#xA;        ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;LEFT(FILE_NUMBER, 2) as NEW_REGION_CODE,&#xA;&#x9;&#x9;LEFT(LEGACY_FILE_NUMBER, 2) AS OLD_REGION_CODE&#xA;    FROM &#xA;        dbo.PIMS_ACQUISITION_FILE&#xA;    WHERE &#xA;       APP_CREATE_USERID = 'PAIMS_PIMS_ACQUISITION'&#xA;&#x9;   AND ACQUISITION_FILE_ID NOT IN ( Select ACQUISITION_FILE_ID FROM etl.CASE2_ALPHABETS )&#xA;&#x9;   --AND ACQUISITION_FILE_ID NOT IN ( Select ACQUISITION_FILE_ID FROM etl.CASE3_SINGLE_DIGITS )&#xA;&#x9;   AND ACQUISITION_FILE_ID NOT IN ( Select ACQUISITION_FILE_ID FROM etl.CASE5_ALPHABET_R )&#xA;&#x9;   AND ACQUISITION_FILE_ID NOT IN ( Select ACQUISITION_FILE_ID FROM etl.CASE4_SUFFIX_NULL )&#xA;)&#xA;--  SELECT * FROM CASE6 WHERE   FILE_NO =100144 AND REGION_CODE = 2 ORDER BY REGION_CODE, FILE_NO&#xA;&#xA;-- Group and filter FILE_NO values with more than one entry&#xA;,group_file_no AS (&#xA;    SELECT &#xA;&#x9;REGION_CODE,&#xA;        FILE_NO, &#xA;        COUNT(1) AS c &#xA;    FROM &#xA;        CASE6 &#xA;    GROUP BY &#xA;        REGION_CODE, FILE_NO &#xA;    HAVING &#xA;        COUNT(1) &gt; 1&#xA;)&#xA;&#xA;--SELECT * FROM group_file_no WHERE FILE_NO = 100144&#xA;&#xA;--SELECT * FROM group_file_no ORDER BY REGION_CODE, file_no&#xA;-- Use LEFT JOIN and IS NULL instead of NOT IN for better performance&#xA;,NUMBERED_FILES AS &#xA;(&#xA;    SELECT&#xA;&#x9;&#x9;CASE6.REGION_CODE,&#xA;        CASE6.FILE_NO, &#xA;        CASE6.FILE_NUMBER,&#xA;        CASE6.ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;CASE6.NEW_REGION_CODE,&#xA;&#x9;&#x9;CASE6.OLD_REGION_CODE,&#xA;        ROW_NUMBER() OVER (PARTITION BY CASE6.REGION_CODE, CASE6.FILE_NO ORDER BY CASE6.FILE_NUMBER) AS NewSuffixNumber&#xA;    FROM &#xA;        CASE6&#xA; &#xA;    WHERE &#xA;&#x9;&#x9;CASE6.NEW_REGION_CODE &lt;&gt; CASE6.OLD_REGION_CODE AND&#xA;         CASE6.FILE_NO IN ( SELECT FILE_NO FROM group_file_no )&#xA;)&#xA;&#xA;--SELECT * FROM NUMBERED_FILES &#xA;--&#x9;WHERE   FILE_NO =100144 AND REGION_CODE = 2 &#xA;--ORDER BY REGION_CODE, FILE_NO&#xA; &#xA;-- Final selection&#xA;SELECT DISTINCT&#xA;&#x9;UF.REGION_CODE,&#xA;    UF.FILE_NO, &#xA;    UF.FILE_NUMBER, &#xA;    UF.ACQUISITION_FILE_ID,&#xA;    CASE &#xA;        WHEN UF.NewSuffixNumber = 1 THEN NULL  -- Parent file (Updated_FILE_NUMBER = &quot;01&quot;)&#xA;        ELSE PF.ACQUISITION_FILE_ID           -- Child file points to the ACQUISITION_FILE_ID of the parent file&#xA;    END AS PRNT_ACQUISITION_FILE_ID&#xA;into etl.CASE6_REMAINING&#xA;FROM &#xA;    NUMBERED_FILES UF&#xA;LEFT JOIN &#xA;    NUMBERED_FILES PF &#xA;    ON UF.FILE_NO = PF.FILE_NO AND UF.REGION_CODE = PF.REGION_CODE&#xA;    AND PF.NewSuffixNumber = 1  -- Join to find the parent file with Updated_FILE_NUMBER = &quot;01&quot;&#xA;&#xA;ORDER BY &#xA;    UF.FILE_NUMBER;&#xA;&#xA; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
