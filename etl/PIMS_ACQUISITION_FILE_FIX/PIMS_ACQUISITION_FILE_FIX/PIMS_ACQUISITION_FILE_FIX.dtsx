          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="--CASE 2 UPDATE&#xA;&#x9;UPDATE A&#xA;&#x9;SET A.FILE_NUMBER = B.Updated_FILE_NUMBER, &#xA;&#x9;&#x9;A.PRNT_ACQUISITION_FILE_ID = B.PRNT_ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;A.APP_LAST_UPDATE_USERID = 'PIMS_ETL_USER'&#xA;&#x9;&#x9;,A.CONCURRENCY_CONTROL_NUMBER=A.CONCURRENCY_CONTROL_NUMBER+1&#xA;&#x9;FROM &#xA;&#x9;&#x9;--etl.PIMS_ACQUISITION_FILE A&#xA;&#x9;&#x9;dbo.PIMS_ACQUISITION_FILE A&#xA;&#x9;JOIN etl.CASE2_ALPHABETS B ON A.ACQUISITION_FILE_ID = B.ACQUISITION_FILE_ID;&#xA;&#xA;&#xA;-- CASE 4 UPDATE&#xA;&#x9;UPDATE A&#xA;&#x9;SET A.FILE_NUMBER = B.FILE_NUMBER, &#xA;&#x9;&#x9;A.PRNT_ACQUISITION_FILE_ID = B.PRNT_ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;A.APP_LAST_UPDATE_USERID = 'PIMS_ETL_USER'&#xA;&#x9;&#x9;,A.CONCURRENCY_CONTROL_NUMBER=A.CONCURRENCY_CONTROL_NUMBER+1&#xA;&#x9;FROM &#xA;&#x9;&#x9;--etl.PIMS_ACQUISITION_FILE A&#xA;&#x9;&#x9;dbo.PIMS_ACQUISITION_FILE A&#xA;&#x9;JOIN etl.CASE4_SUFFIX_NULL B ON A.ACQUISITION_FILE_ID = B.ACQUISITION_FILE_ID;&#xA;&#xA;&#xA;-- CASE 5 UPDATE&#xA;&#x9;UPDATE A&#xA;&#x9;SET A.FILE_NUMBER = B.Updated_FILE_NUMBER, &#xA;&#x9;&#x9;A.PRNT_ACQUISITION_FILE_ID = B.PRNT_ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;A.APP_LAST_UPDATE_USERID = 'PIMS_ETL_USER'&#xA;&#x9;&#x9;,A.CONCURRENCY_CONTROL_NUMBER=A.CONCURRENCY_CONTROL_NUMBER+1&#xA;&#x9;FROM &#xA;&#x9;&#x9;--etl.PIMS_ACQUISITION_FILE A&#xA;&#x9;&#x9;dbo.PIMS_ACQUISITION_FILE A&#xA;&#x9;JOIN etl.CASE5_ALPHABET_R B ON A.ACQUISITION_FILE_ID = B.ACQUISITION_FILE_ID;&#xA;&#xA;-- CASE 6 UPDATE ( CASE 1 Equivalent )&#xA;&#x9;UPDATE A&#xA;&#x9;SET A.PRNT_ACQUISITION_FILE_ID = B.PRNT_ACQUISITION_FILE_ID,&#xA;&#x9;&#x9;A.APP_LAST_UPDATE_USERID = 'PIMS_ETL_USER'&#xA;&#x9;&#x9;,A.CONCURRENCY_CONTROL_NUMBER=A.CONCURRENCY_CONTROL_NUMBER+1&#xA;&#x9;FROM &#xA;&#x9;&#x9;--etl.PIMS_ACQUISITION_FILE A&#xA;&#x9;&#x9;dbo.PIMS_ACQUISITION_FILE A&#xA;&#x9;JOIN etl.CASE6_REMAINING B ON A.ACQUISITION_FILE_ID = B.ACQUISITION_FILE_ID;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tas