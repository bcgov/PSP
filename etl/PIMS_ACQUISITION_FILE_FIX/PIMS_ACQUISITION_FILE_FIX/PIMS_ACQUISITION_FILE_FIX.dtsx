        <SQLTask:SqlTaskData
          SQLTask:Connection="{2409A3DC-5AAA-407E-9B0B-7CDE117FABDB}"
          SQLTask:SqlStatementSource="WITH CASE4 AS&#xA;(&#xA;&#x9;SELECT FILE_NUMBER, COUNT(1) AS X FROM etl.PIMS_ACQUISITION_FILE_BKP&#xA;&#x9;WHERE FILE_NUMBER = LEGACY_FILE_NUMBER&#xA;&#xA;&#x9;GROUP BY FILE_NUMBER&#xA;&#x9;HAVING COUNT(1) &gt; 1&#xA;)&#xA;&#xA;SELECT &#xA;&#x9;PAIMS_ACQUISITION_FILE_ID,&#xA;&#x9;ACQUISITION_FILE_ID, &#xA;&#x9;FILE_NO, &#xA;&#x9;FILE_NUMBER, &#xA;&#x9;LEGACY_FILE_NUMBER,  &#xA;&#x9;[ACQUISITION_FILE_STATUS_TYPE_CODE],&#xA;&#x9;LEFT(FILE_NUMBER, 2) as REGION_CODE,LEFT(LEGACY_FILE_NUMBER, 2) AS OLD_REGION_CODE, &#xA;&#x9;PRNT_ACQUISITION_FILE_ID, NULL AS SUFFIX&#xA;INTO etl.CASE4_SUFFIX_NULL&#xA;FROM&#x9;dbo.PIMS_ACQUISITION_FILE&#xA;WHERE &#xA;&#x9;FILE_NUMBER IN ( Select FILE_NUMBER FROM CASE4 )&#xA;&#xA;ORDER BY FILE_NO&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
