      DTS:DTSID="{bae35f9c-1667-48a2-9ac1-2269792a3e51}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="PIMS_LEASE_NOTE"
