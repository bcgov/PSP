    "hosted-git-info": "4.0.2",
    "glob-parent": "5.1.2",
    "ws": "6.2.2",
    "prismjs": "1.24.1",
    "css-what": "5.0.1",
    "dns-packet": "5.3.0",
    "trim-newlines": "4.0.2"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "react-app-rewired start",
    "build": "cross-env CI=true GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "test": "tsc && react-app-rewired test --ci --watchAll=false --runInBand",
    "test:watch": "react-app-rewired test",
    "coverage": "react-app-rewired test --ci --coverage --testResultsProcessor jest-sonar-reporter --watchAll=false --runInBand",
    "update-snapshots": "react-app-rewired test --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "pre-push:check": "npm run check && npm run lint",
    "docu:server": "npx styleguidist server",
    "prepare": "cd .. && husky install frontend/.husky",
    "pretty-quick": "pretty-quick"
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/coverage/**",
      "!<rootDir>/public/**",
      "!<rootDir>/build/**",
      "!<rootDir>/src/serviceWorker.**",
      "!<rootDir>/src/setupProxy.*",
      "!<rootDir>/src/setupTests.*"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      "<rootDir>/src/index.tsx",
      ".mock.ts",
      ".ignore.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 45,
        "functions": 45,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
